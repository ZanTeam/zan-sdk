    "lint:script": "eslint --ext .js,.jsx,.ts,.tsx --fix --quiet ./src",
    "prepare": "husky"
  },
  "keywords": [
    "zan",
    "sdk",
    "node service"
  ],
  "authors": [],
  "license": "MIT",
  "files": [
